{
  "name": "Love_Will_Seed",
  "description": "Seed to build single page apps",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "gWatch": "gulp watch",
    "install": "napa",
    "build": "gulp registerModules && browserify  -t brfs app/app.js > dist/bundle.js",
    "start": "npm run build && cd dist && w3",
    "start-hey": "node hey.js",
    "push": "git push origin master && git push backup master",
    "watch": "gulp registerModules && watchify -t node-lessify app/app.js -o dist/bundle.js",
    "deploy": "cp -R ~/LoveWillApps/Seedlings/simplesend./dist/ ~/LoveWillApps/Seedlings/simplesend./parse/public/ && cd ~/LoveWillApps/Seedlings/simplesend./parse && parse develop 'Simple Send'"
  },
  "napa": {
    "angular": "angular/bower-angular",
    "angular-route": "angular/bower-angular-route",
    "angular-resource": "angular/bower-angular-resource",
    "angular-sanitize": "angular/bower-angular-sanitize",
    "ui-router": "angular-ui/ui-router"
  },
  "devDependencies": {
    "bip38": "^1.3.0",
    "bip39": "^2.1.2",
    "brfs": "^1.4.0",
    "browserify": "^10.2.4",
    "bulk-require": "^0.2.1",
    "bulkify": "^1.1.1",
    "crypto-js": "^3.1.5",
    "cssify": "^0.6.0",
    "express": "^4.13.1",
    "gulp": "^3.9.0",
    "gulp-concat": "^2.6.0",
    "gulp-if": "^1.2.5",
    "gulp-livereload": "^3.8.0",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.6",
    "hashids": "^1.0.2",
    "node-parse-api": "^0.3.7",
    "parse-browserify": "^0.1.2",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.2.3"
  },
  "dependencies": {
    "bip38": "^1.3.0",
    "bip39": "^2.1.2",
    "brfs": "^1.4.0",
    "browserify": "^10.2.4",
    "bulk-require": "^0.2.1",
    "bulkify": "^1.1.1",
    "crypto-js": "^3.1.5",
    "hashids": "^1.0.2",
    "node-parse-api": "^0.3.7",
    "parse-browserify": "^0.1.2"
  }
}
